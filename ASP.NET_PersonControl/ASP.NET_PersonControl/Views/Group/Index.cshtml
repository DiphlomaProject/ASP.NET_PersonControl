@model ASP.NET_PersonControl.Models.Groups
@{
    ViewBag.Title = "GroupManager";
}

<h2>Group Manager</h2>
<table id="group" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Owner</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
                <td>@Model.Id</td>
                <td>@Model.Title</td>
                <td>@Model.Owner</td>
                <td>@Model.Description</td>
        </tr>
   
    </tbody>
</table>
<br />
<button class="btn btn-primary" onclick="@("window.location.href='" + @Url.Action("Index", "Home") + "'");">Back</button>
